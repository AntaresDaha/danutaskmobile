Ext.define('CDS.view.subview.ColorPatterns', {
    singleton: true,
    colors: [ "#7F9600", "#E6CB00", "#a61120", "#115fa6", "#ff8809", "#ffd13e", "#a61187", "#24ad9a", "#7c7474", "#a66111"],
    barcolors: [ "#E39505", "#E39505", "#E39505", "#E39505"],

    getBaseColors: function (index) {
        if (index == null) {
            return this.colors.slice();
        } else {
            return this.colors[index];
        }
    },
    getBarBaseColors: function (index) {
        if (index == null) {
            return this.barcolors.slice();
        } else {
            return this.barcolors[index];
        }
    }
});